{
    "name": "arifnd/laraset",
    "description": "Setup laravel package made easy.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://github.com/arifnd/laraset",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/arifnd/laraset/issues",
        "source": "https://github.com/arifnd/laraset"
    },
    "authors": [
        {
            "name": "Ari Effendi",
            "email": "lifioting@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laminas/laminas-text": "^2.9",
        "nunomaduro/termwind": "^1.13"
    },
    "require-dev": {
        "laravel-zero/framework": "^9.1.3",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.3",
        "timwassenburg/laravel-service-generator": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/laraset"]
}
